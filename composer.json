{
    "name": "codestacx/uploaders",
    "description": "This package is used to upload files to the server",
    "version":     "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JackSparroow",
            "email": "muhammadatif.pucit@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Codestacx\\Uploaders\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codestacx\\Uploaders\\UploaderServiceProvider"
            ],
            "aliases": {
                "Fileuploader" : "Codestacx\\Uploaders\\Facades\\UploaderFacade"

            }
        }
    },

    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0"
    },
    "minimum-stability": "dev"
}
